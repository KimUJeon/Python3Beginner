# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean

S1 = {1, 3}
S2 = {2, 3}

print(S1.intersection(S2))
print(S1.union(S2))
print(S1.difference(S2))

S1.add('goorm')
print(S1)

odd_list = [5, 'odd']
S2.update(odd_list)
print(S2)

S2.remove(5)
print(S2)

'''
{3}
{1, 2, 3}
{1}
{'goorm', 1, 3}
{'odd', 2, 3, 5}
{'odd', 2, 3}
'''
